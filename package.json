{
  "name": "discovery",
  "version": "0.0.1",
  "description": "An npm script to help diagonose meteor projects for vulnerabilities",
	"scripts": {
		"lint": "eslint ./ --cache --ignore-pattern .gitignore",
		"lint-staged": "lint-staged",
		"start": "yarn prepublish; node lib/index.js",
		"update": "git pull origin master",
		"precommit-msg": "echo 'Pre-commit checks...' && exit 0",
		"compile": "gulp build & babel --presets es2015 --stage 0 --optional runtime src --out-dir lib",
		"watch": "gulp watch & babel --watch --presets es2015 --stage 0 --optional runtime src --out-dir lib",
		"prepublish": "yarn run compile",
		"storybook": "start-storybook -p 9001",
		"test": "echo 'still do to'"
	},
  "main": "lib/index.js",
	"lint-staged": {
		"**/*.js": [
			"eslint --fix",
			"git add"
		]
	},
	"pre-commit": [
		"precommit-msg",
		"lint-staged"
	],
	"keywords": [],
	"dependencies": {
	},
	"devDependencies": {
		"babel-cli": "^6.6.5",
		"babel-core": "^6.6.1",
		"babel-eslint": "^7.0.0",
		"babel-loader": "^6.2.4",
		"babel-plugin-lodash": "^3.1.4",
		"babel-preset-es2015": "^6.6.0",
		"chai": "^3.5.0",
		"eslint": "^3.6.1",
		"eslint-config-airbnb": "^12.0.0",
		"eslint-plugin-import": "^1.16.0",
		"eslint-plugin-jsx-a11y": "^2.2.2",
		"eslint-plugin-react": "^6.3.0",
		"file-loader": "^0.9.0",
		"gulp": "^3.9.1",
		"lint-staged": "^3.0.3",
		"pre-commit": "^1.1.3"
	},
	"author": "Hugo Michalski <hugo@side.co>",
	"license": "MIT"
}
